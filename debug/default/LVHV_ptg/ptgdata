######################## Operation Info ########################
o = Opinfo("write",0x0)
t = Tinfo()
t.start = 0x0
t.end = 0x1fff
t.data = "write_Pgm_st_0x0_op4_0.dat"
t.type = "Pgm"
t.mask = 0x0
t.crc = 0x7a5b
o.addTI(t)
t = Tinfo()
